{
  "name": "@researchdatabox/raido-openapi-generated-node",
  "version": "0.0.1",
  "description": "Convenience lib for the autogenerated client code using https://openapi-generator.tech",
  "main": "dist/api.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pregenerate": "npm install @openapitools/openapi-generator-cli -g",
    "generate": "openapi-generator-cli generate -i https://github.com/au-research/raido-v2/raw/main/api-svc/idl-raid-v2/src/raido-openapi-3.0.yaml -g typescript-node -o ./src; node_modules/.bin/tsc"
  },
  "author": "QCIF Engineering",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/request": "^2.48.8",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }  
}
